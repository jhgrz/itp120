package mod2wolffexamplesClasses;

public class Flight {
	private String name;
	private String flightNumber;
	private String origin;
	private String destination;
	private double cost;
	
	//No Argument Constructor
	public Flight ()
	{
	}
	
	//Partial Constructor First 4
	public Flight ()
	
	//Full Constructor
	public Flight (String n, String f, String o, String d)
	{
		name = n;
		flightNumber = f;
		origin = o;
		destination = d;
		cost = "";
	}
	
	//Getters Setters
	
	
	
	


}
