package mod2wolffexamplesDecisions;
import java.util.Scanner;

public class Slots {

	public static void main(String[] args) {
	Scanner scan = new Scanner(System.in);
	
	int x = 5;
	int y = 5;
	int z = 5;
	
	System.out.printf("Your Randomly Generated Slot Numbers are: %n%15s %5s %5s", x, y, z);
	
	if (x == y && y == z && z==x) 
		System.out.println("%n%nYOU WON IT ALL, 3 numbers matched!");
	
	else if (x == y)
		System.out.printf("%n%nHey two numbers matched, so close!");
	
	else if (y == z)
		System.out.printf("%n%nHey two numbers matched, so close!");
	
	else if (x == z)
		System.out.printf("%n%nHey two numbers matched, so close!");
	
	
		
		
		
		
		
	}

}
